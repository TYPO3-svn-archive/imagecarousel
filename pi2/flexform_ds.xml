								<size>5</size>
								<max>5</max>
								<default></default>
								<eval>nospace</eval>
							</config>
						</TCEforms>
					</minScale>
					<xRadius>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:imagecarousel/locallang_db.xml:tt_content.pi_flexform.xRadius</label>
							<config>
								<type>input</type>
								<size>5</size>
								<max>5</max>
								<default></default>
								<eval>nospace</eval>
							</config>
						</TCEforms>
					</xRadius>
					<yRadius>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:imagecarousel/locallang_db.xml:tt_content.pi_flexform.yRadius</label>
							<config>
								<type>input</type>
								<size>5</size>
								<max>5</max>
								<default></default>
								<eval>nospace</eval>
							</config>
						</TCEforms>
					</yRadius>
					<xPos>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:imagecarousel/locallang_db.xml:tt_content.pi_flexform.xPos</label>
							<config>
								<type>input</type>
								<size>5</size>
								<max>5</max>
								<default></default>
								<eval>nospace</eval>
							</config>
						</TCEforms>
					</xPos>
					<yPos>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:imagecarousel/locallang_db.xml:tt_content.pi_flexform.yPos</label>
							<config>
								<type>input</type>
								<size>5</size>
								<max>5</max>
								<default></default>
								<eval>nospace</eval>
							</config>
						</TCEforms>
					</yPos>
				</el>
			</ROOT>
		</control>
		<buttons>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:imagecarousel/locallang_db.xml:tt_content.pi_flexform.sheet_buttons</sheetTitle>
					<cshFile>LLL:EXT:imagecarousel/locallang_csh_flexform.xml</cshFile>
				</TCEforms>
				<type>array</type>
				<el>
