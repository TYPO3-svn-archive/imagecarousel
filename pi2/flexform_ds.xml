									<href>
										<TCEforms>
											<label>LLL:EXT:imagecarousel/locallang_db.xml:tt_content.pi_flexform.href</label>
											<config>
												<type>input</type>
												<size>15</size>
												<max>255</max>
												<eval>trim</eval>
												<wizards>
