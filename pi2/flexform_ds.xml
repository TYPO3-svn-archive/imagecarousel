						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:imagecarousel/locallang_db.xml:tt_content.pi_flexform.speed</label>
							<config>
								<type>input</type>
								<size>5</size>
								<max>5</max>
